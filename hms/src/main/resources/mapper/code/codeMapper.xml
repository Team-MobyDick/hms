<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mobydick.hms.code.dao.CodeDAO">

    <!-- 객실 코드와 이름을 조회 -->
    <select id="getRoomCode" resultType="com.mobydick.hms.code.vo.CodeVO">
        SELECT
            CODE_ID,
            CODE_NAME,
            FLAG,
            CREATED_DATE,
            CREATED_ID,
            UPDATED_DATE,
            UPDATED_ID
        FROM TB_CODE
        WHERE
            CODE_ID LIKE 'TP%'
    </select>

</mapper>